{
  "projectType": "library",
  "root": "libs/cree/library/feature",
  "sourceRoot": "libs/cree/library/feature/src",
  "prefix": "portal",
  "targets": {
    "test": {
      "executor": "@nrwl/jest:jest",
      "outputs": ["coverage/libs/cree/library/feature"],
      "options": {
        "jestConfig": "libs/cree/library/feature/jest.config.js",
        "passWithNoTests": true
      }
    },
    "lint": {
      "executor": "@nrwl/linter:eslint",
      "options": {
        "lintFilePatterns": ["libs/cree/library/feature/src/**/*.ts", "libs/cree/library/feature/src/**/*.html"]
      }
    }
  },
  "tags": ["type:feature", "scope:library"]
}
